def setup():
    size(600, 400)
    global x
    global y
    global x2
    global y2
    x = width / 2
    y = height / 2
    x2 = 600
    y2 = 400
def draw():
    global x
    global y
    global x2
    global y2
    background(100)
    stroke(floor(random(0,255)) , floor(random(0,255)), floor(random(0,255)))
    strokeWeight(5)
    fill(floor(random(0,255)) , floor(random(0,255)), floor(random(0,255)))
    ellipse(x, y, x2 , y2)
    x = x + 0
    y = y + 0
    x2 = x2 - 2
    y2 = y2 - 2
